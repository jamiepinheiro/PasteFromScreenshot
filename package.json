{
    "name": "paste-from-screenshot",
    "displayName": "Paste from Screenshot",
    "description": "Paste the text content of a screenshot",
    "version": "0.0.1",
    "publisher": "jamiepinheiro",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.pasteFromScreenshot"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.pasteFromScreenshot",
                "title": "Paste From Screenshot"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/request": "^2.47.1",
        "@types/request-promise-native": "^1.0.15",
        "request": "^2.87.0",
        "request-promise-native": "^1.0.5"
    }
}
